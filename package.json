{
  "name": "test-results",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lintfix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "preview": "npx vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test -c playwright.config.ts --retries=1",
    "test:ci": "npx playwright test -c playwright.config.ts --retries=1 --workers=1 && npx nyc report --reporter=text",
    "test:e2e:ui": "npx playwright test --ui",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "fast-xml-parser": "latest",
    "html2canvas": "latest",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.546.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.3",
    "autoprefixer": "latest",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "nyc": "^17.1.0",
    "postcss": "latest",
    "tailwindcss": "3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-istanbul": "^7.2.0",
    "vitest": "^3.2.3"
  }
}
